<?xml version="1.0" encoding="utf-8" ?>
<cmds>
    <cmd name="write_file">
        <fopen handle="@handle" file="@file" mode="w"/>
        <fwrite handle="@handle" data="@data"/>
        <fclose handle="@handle"/>
    </cmd>
    <cmd name="clear_int64">
        <setmem memory="@memory" offset="0" byte="0"/>
        <setmem memory="@memory" offset="1" byte="0"/>
        <setmem memory="@memory" offset="2" byte="0"/>
        <setmem memory="@memory" offset="3" byte="0"/>
        <setmem memory="@memory" offset="4" byte="0"/>
        <setmem memory="@memory" offset="5" byte="0"/>
        <setmem memory="@memory" offset="6" byte="0"/>
        <setmem memory="@memory" offset="7" byte="0"/>
    </cmd>
</cmds>