<?xml version="1.0" encoding="utf-8" ?>
<cmds>
    <cmd name="write_file">
        <fopen handle="@handle" file="@file" mode="w"/>
        <fwrite handle="@handle" data="@data"/>
        <fclose handle="@handle"/>
    </cmd>
    <cmd name="add32">
        <add type="char" a="va" b="vb" out="va"/>
    </cmd>
</cmds>